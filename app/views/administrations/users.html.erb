<%= @users.count %>

<% if user_signed_in? and current_user.admin? %>

	<div class="tickets-list" style="text-align: left;">
		<% @users.each do |user| %>
		<ul>
			<li>Title: <%= user.username %></li>
			<li>Favorited: <%= Follow.where("user_id = ?", user.id).count %></li>
			<li>Created: <%= user.created_at %> </li>
			<li>Updated: <%= user.updated_at %></li>
			<li>Edit: <%= link_to 'Edit', edit_user_path(user) %></li>
			<li>Destroy: <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></li>
		</ul>
		<% end %>
	</div>




<% else %>
	Barra el3ab b3id
<% end %>